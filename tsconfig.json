{
  "compileOnSave": false,
  "compilerOptions": {
    "jsx": "react",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "target": "es5",
    "lib": ["es2016", "dom"],
    "baseUrl": ".",
    "esModuleInterop": true,
    "paths": {
      "react-oidc-context-params-redirect": ["packages/context/dist"],
      "react-oidc-core-params-redirect": ["packages/core/dist"]
    }
  }
}
